class main1 {
    public static void main(String []args){
        String first="Blue";
        System.out.println("First String:"+first);
        String second="Whale";
        System.out.println("Second String:"+second);
        String joinedString=first.concat(second);
        
        System.out.println("Joined String:"+ joinedString);
        
    }
    
}
